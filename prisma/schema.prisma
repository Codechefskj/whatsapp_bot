generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ===================== WHATSAPP MESSAGE ===================== 
model WhatsAppMessage {
  id        Int      @id @default(autoincrement())
  from      String
  text      String
  messageId String   @unique
  createdAt DateTime @default(now())
}